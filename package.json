{
  "name": "ai-meetings-assistant-app",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "concurrently \"npm:dev:web\" \"npm:dev:server\"",
    "dev:web": "npm --prefix apps/web run dev",
    "dev:server": "npm --prefix packages/server run dev",
    "lint": "eslint \"**/*.{ts,tsx,js,jsx}\" --max-warnings=0",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "test": "jest --coverage"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "nyc": "^15.1.0",
    "prettier": "^3.1.0",
    "supertest": "^6.3.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5"
  }
}
